package com.in28minutes.springboot.learnspringboot;

public class Course {
	private long id;
	private String name;
	private String author;
	
	
	
	
	//Getters
	
	public long getId() {
		return id;
	}


	public String getName() {
		return name;
	}


	public String getAuthor() {
		return author;
	}



	@Override
	public String toString() {
		return "Course [id=" + id + ", name=" + name + ", author=" + author + "]";
	}
	
	//Constructor
	//Getters
	//toString 
}
